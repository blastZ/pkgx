{
  "compileOnSave": false,
  "compilerOptions": {
    "declaration": false,
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "importHelpers": true,
    "target": "ESNext",
    "module": "NodeNext",
    "outDir": "./dist",
    "removeComments": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
  },
}